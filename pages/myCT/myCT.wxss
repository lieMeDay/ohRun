.page{
  width: 100%;
  min-height: 100%;
  position: absolute;
  background-color: #f6f6f6;
}
.companyWrap{
  width: 100%;
  background-color: #fff;
  height: 140rpx;
  padding: 20rpx 0;
}
.cBox{
  width: 95%;
  height: 140rpx;
  margin: 0 auto;
  display: flex;
}
.biglogo{
  width: 140rpx;
  height: 140rpx;
  margin-right: 2.5%;
  background-color: #ccc;
}
/* .cTit{
  display: flex;
  justify-content: space-between;
} */
.cMsg{
  width: calc(100% - 140rpx - 2.5%);
}
.ctcon{
  float: left;
  width: calc(100% - 90rpx);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.ctcon.nrxt{
  width: 100%;
}
.lcnr{
  margin-top: 30rpx;
}
.lcflex{
  display: flex;
  flex-direction: column;
  /* align-items: center; */
  justify-content: space-around;
}
.lcnr:last-child{
  margin-bottom: 30rpx;
}
.setting{
  float: right;
  width: 80rpx;
  text-align: center;
}
.cDate{
  font-size: 30rpx;
}
.listWrap{
  width: 100%;
  overflow: hidden;
  background-color: #fff;
  margin-top: 20rpx;
}
.listTit{
  line-height: 80rpx;
  display: flex;
  justify-content: space-around;
}
.bline{
  height: 6rpx;
  background-color: transparent;
}
.bline.act{
  background-color: #000;
}
.listCon{
  min-height: 280rpx;
}
.lc{
  width: 95%;
  margin: 0 auto;
}
.footer{
  width: 100%;
  position: fixed;
  bottom: 0;
  z-index: 10;
  display: flex;
  justify-content: space-around;
  height: 100rpx;
  line-height: 100rpx;
  background-color: yellowgreen;
  color: #fff;
}

/* 无加入团队显示 创建或加入 */
.pBox{
  width: 95%;
  overflow: hidden;
  margin: 0 auto;
  text-align: center;
}
.pCon{
  margin-top: 100rpx;
}
.plogo{
  width: 140rpx;
  height: 140rpx;
  background-color: #cccccc;
  margin: 0 auto;
  margin-bottom: 30rpx;
}
.pTxt{
  font-size: 30rpx;
  color: #999999;
}
.pBtn{
  margin-top: 50rpx;
}
.btn{
  width: 400rpx;
  height: 80rpx;
  margin: 0 auto;
  background-color: #fff;
  border: 2rpx solid #000;
  line-height: 80rpx;
  border-radius: 10rpx;
  margin-bottom: 50rpx;
}